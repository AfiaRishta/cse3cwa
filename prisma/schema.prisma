generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model Session {
  id             Int      @id @default(autoincrement())
  startedAt      DateTime @default(now())
  minutes        Int
  doneAlt        Boolean  @default(false)
  doneValidation Boolean  @default(false)
  doneLogin      Boolean  @default(false)
  doneSecureDb   Boolean  @default(false)
  doneTitleColor Boolean  @default(false)
}

